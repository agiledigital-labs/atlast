{
  "name": "atlast",
  "version": "1.0.0",
  "description": "Interactive CLI tool for managing Atlassian resources.",
  "main": "index.js",
  "private": "true",
  "author": "Agile Digital <hello@agiledigital.com.au> (https://agiledigital.com.au)",
  "license": "MIT",
  "scripts": {
    "build": "NODE_ENV=production rollup -c --environment BUILD:production",
    "lint": "eslint src --ext .ts,.tsx",
    "start": "NODE_ENV=development rollup -c",
    "watch": "npm run start -- -w",
    "type-coverage": "type-coverage",
    "test": "jest",
    "mutation-test": "stryker run"
  },
  "dependencies": {
    "@types/node-fetch": "^2.6.2",
    "fp-ts": "^2.13.1",
    "jira-client": "^8.2.2",
    "yargs": "^17.6.2"
  },
  "devDependencies": {
    "@relmify/jest-fp-ts": "^2.0.2",
    "@stryker-mutator/core": "6.1.2",
    "@stryker-mutator/jest-runner": "6.1.2",
    "@stryker-mutator/typescript-checker": "6.1.2",
    "@types/clear": "0.1.2",
    "@types/clui": "0.3.1",
    "@types/jest": "29.2.5",
    "@types/jira-client": "^7.1.5",
    "@types/node": "18.7.13",
    "@types/yargs": "17.0.11",
    "@typescript-eslint/eslint-plugin": "5.38.0",
    "@typescript-eslint/parser": "5.38.0",
    "eslint": "8.23.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-typed-fp": "3.3.0",
    "eslint-plugin-functional": "4.4.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "26.9.0",
    "eslint-plugin-jsdoc": "39.3.6",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-sonarjs": "0.15.0",
    "eslint-plugin-total-functions": "6.0.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "fast-glob": "^3.2.12",
    "io-ts": "^2.2.18",
    "jest": "28.1.3",
    "prettier": "2.7.1",
    "rollup": "2.78.1",
    "rollup-plugin-cleanup": "3.2.1",
    "rollup-plugin-delete": "2.0.0",
    "rollup-plugin-typescript2": "0.33.0",
    "ts-jest": "28.0.8",
    "tsconfig-paths-webpack-plugin": "4.0.0",
    "type-coverage": "2.22.0",
    "typescript": "4.8.3"
  },
  "typeCoverage": {
    "atLeast": 100,
    "ignoreCatch": false,
    "strict": true,
    "detail": true
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 14,
        "lines": 37,
        "statements": 40
      }
    },
    "setupFilesAfterEnv": [
      "@relmify/jest-fp-ts"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agiledigital-labs/atlast.git"
  },
  "bugs": {
    "url": "https://github.com/agiledigital-labs/atlast/issues"
  },
  "homepage": "https://github.com/agiledigital-labs/atlast#readme"
}
